        xmlns:local="clr-namespace:WpfApp1" xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <dxg:GridControl>
            <dxg:GridControl.Columns>
                <dxg:GridColumn Header="사용자 ID" FieldName="UserID" ReadOnly="true" Width="100">
                </dxg:GridColumn>
            </dxg:GridControl.Columns>
        </dxg:GridControl>
    </Grid>
</Window>
